<!-- Content Block
============================================-->
<section class="content-block default-bg">

  <!-- Container -->
  <div class="container no-pad-t">

    <!-- Row -->
    <div class="row">

      <!-- Main Col -->
      <div class="main-col col-sm-8 col-md-8 mgb-30-xs">
      
        <h4>About Becoming a Gather Artist</h4>
        <p>If you are an artist who is interested in joining the Gather Art Registry, we will be hosting a selection process each year in the early spring. Our next call deadline is ________(date).  Please follow the call for entries procedure below. Weâ€™re excited to get to know you and your work!</p>
        
        <!-- Accordion -->
        <div class="panel-group" id="accordion">
        	
        	<%= @content.each do |content| %>
          <!-- Panel -->
          <div class="panel panel-default">
            <!-- Heading -->
            <div class="panel-heading">
              <h4 class="panel-title">
              <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                <%= content.title %>
              </a>
              </h4>
            </div>
            <!-- /Heading -->
            
            <!-- Collapse -->
            <div id="collapseOne" class="panel-collapse collapse in">
              <div class="panel-body">
              <%= content.body %>
              </div>
            </div>
            <!-- /Collapse -->
            
          </div>
          <!-- /Panel -->
          <% end %>
          
        </div>
        <!-- /Accordion -->
        
      </div>
      <!-- /Main Col -->
      
      <%= render 'side_bar' %>

    </div>
    <!-- /Row -->
  
  </div>
  <!-- /Container -->
  
</section>
<!-- /Content Block
============================================-->

